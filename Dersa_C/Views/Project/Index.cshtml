@using System.Data
@model DataTable

@{
    ViewBag.Title = "ObjectType";
}


<h2>Object types</h2>

<table>
    <tr>
        <th>
            <strong>class name</strong>
        </th>
        <th></th>
    </tr>

    @foreach (DataRow item in Model.Rows)
    {
        <tr>
            <td style="padding:2px 5px">
                @Html.DisplayFor(modelItem => item["class_name"])
            </td>
            <td style="padding:2px 5px">
                @Html.ActionLink("View", "Display", new { class_name = item["class_name"] })
            </td>
        </tr>
    }

</table>

